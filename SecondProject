from math import *

file  = pickAFile()
sound = makeSound(file)
sample = getSamples(sound)
arr = []
height = getLength(sound) / 1000
pic = makeEmptyPicture(1000, height)

for sample in range(0, getLength(sound)):
  if sound.getSampleValue(sample) == 0:
    arr.append(1)
  elif sound.getSampleValue(sample) < 0:
    arr.append(2)
  else:
    arr.append(3)

